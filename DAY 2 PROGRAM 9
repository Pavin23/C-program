Vowels or Consonants

#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main() {
    char ch;
    scanf(" %c", &ch);

    if ((ch>='A'&&ch<='Z')||(ch>='a'&&ch<='z')) {
        if (ch=='A'||ch=='E'||ch=='I'||ch=='O'||ch=='U'||
            ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u') {
            printf("The Character %c is Vowel", ch);
        } else {
            printf("The Character %c is Consonant", ch);
        }
    } else {
        printf("Invalid Input");
    }

    return 0;
}
