#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    char a[100],value=0;
    scanf("%s",a);
    int len=strlen(a);
    for(int i=0;i<len;i++){
        if(i==0||i%2==0)
        {
            if(a[i]==a[i-2])
            {
                value=1;
            }
            else
            {
                value=0;
            }
        }
        }
    if(value)
    {
        printf("Yes");
    }
    else{
        printf("No");
    }
    return 0;
}
