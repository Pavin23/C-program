PROFIT CALCULATOR\
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

  int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    float pro;
    pro=((a*b)-(a*c))-100;
    printf("Number of copies sold:%d\n",a);
    printf("Cost of each copy:%d\n",b);
    printf("Cost spent by agency on each newspaper:%d\n",c);
    printf("The profit obtained is Rs.%.2f\n",pro);
    return 0;
}

