Automorphic number

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>
int main() {
    int n, square;
    scanf("%d", &n);
    if(n <= 0 || n >= 10) {
        printf("Invalid Input\n");
        return 0;
    }
    square = n * n;
    if(square % 10 == n) {
        printf("Automorphic Number\n");
    } else {
        printf("Not Automorphic Number\n");
    }

    return 0;
}
