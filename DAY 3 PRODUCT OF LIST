
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    int n,pro=1;
    scanf("%d",&n);
    int arr[n];
    for(int i=0;i<n;i++)
    {
        scanf("%d",&arr[i]);
    }
        for(int i=0;i<n;i++)
        {
            pro*=arr[i];
        }
    printf("The Product of list value is %.2f",(float)pro);
    
    return 0;
}
