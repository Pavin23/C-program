P2 - Green Lights


#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

#include <stdio.h>

int main() {
    float speed, distance, time;
    scanf("%f", &speed);
    scanf("%f", &distance);
    scanf("%f", &time);
    float speed_mps = speed / 3600.0;
    float time_required = distance / speed_mps;
    if (time_required < time) {
        printf("Yes\n");
    } else {
        printf("No\n");
    }

    return 0;
}
