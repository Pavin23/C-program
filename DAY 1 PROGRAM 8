Math Functions

#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    float a;
    int b,c,d,e;
    scanf("%f\n",&a);
    scanf("%d\n%d\n%d%d\n",&b,&c,&d);
    printf("%d\n",(int)floor(a));
    printf("%d\n",(int)ceil(a));
      printf("%d\n",(int)sqrt(b));
      printf("%d\n",(int)pow(c,d));
    return 0;
}
