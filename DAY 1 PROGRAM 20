THREE IDIOTS
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   int x1,y1,x2,y2;
    scanf("%d\n%d\n%d\n%d\n",&x1,&y1,&x2,&y2);
    float xm,ym;
    xm=(x1+x2)/2.0;
    ym=(y1+y2)/2.0;
    printf("Binoy's house is located at (%.1f,%.1f)",xm,ym);
    return 0;
}
